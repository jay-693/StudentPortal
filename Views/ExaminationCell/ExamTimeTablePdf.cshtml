@{
    Layout = null;
}
@model IEnumerable<StudentPortal.Models.ExamTimeTable>


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Exam TimeTable List</title>
    <style>
        .TablePartial {
            width: 100%;
            border-collapse: collapse;
        }

            .TablePartial th, .TablePartial td {
                border: 1px solid black;
                padding: 8px;
                text-align: left;
            }

            .TablePartial th {
                background-color: #f2f2f2;
            }
    </style>
</head>
<body>
    <h1>Exam TimeTable</h1>
    <p>Semester: @TempData["Semester"]</p>
    @Html.Partial("_ExamTimeTable")
</body>
</html>